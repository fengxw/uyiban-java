<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uyiban.freshman.mapper.StudentMapper">
    <select id="getStudentById" resultType="com.uyiban.freshman.model.StudentModel">
        select id, name from student where id = #{id}
    </select>
</mapper>